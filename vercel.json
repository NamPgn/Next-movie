{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "https://www.hoathinhtrungquoc.site/(.*)",
      "destination": "https://hoathinhtrungquoc.site/$1",
      "permanent": true
    }
  ]
}
